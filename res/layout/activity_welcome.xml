<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    style="@style/bg_ff">


    <android.support.v4.view.ViewPager
        android:id="@+id/vp_start"
        style="@style/bg_ff"></android.support.v4.view.ViewPager>

    <LinearLayout
        style="@style/bg_fw"
        android:layout_alignBottom="@+id/vp_start"
        android:layout_marginBottom="@dimen/margin_top_10"
        android:gravity="center">

        <ImageView
            android:id="@+id/img_quide_1"
            style="@style/bg_ww"
            android:layout_margin="10dp"
            android:src="@drawable/adware_style_selected" />

        <ImageView
            android:id="@+id/img_quide_2"
            style="@style/bg_ww"
            android:layout_margin="10dp"
            android:src="@drawable/adware_style_default" />

        <ImageView
            android:id="@+id/img_quide_3"
            style="@style/bg_ww"
            android:layout_margin="10dp"
            android:src="@drawable/adware_style_default" />

    </LinearLayout>
</RelativeLayout>

